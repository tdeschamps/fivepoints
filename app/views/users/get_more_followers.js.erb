("<%= j render partial: './shared/user_list', locals: {users: @followers} %>").insertBefore($('#follower-list-modal').find('.clear'));

<% if @followers.next_page  %>
  $('#follower-list-modal').find('.pagination').replaceWith('<%= j will_paginate @followers %>');
<% else %>
  $('#follower-list-modal').find('.see-more').remove();
<% end %>
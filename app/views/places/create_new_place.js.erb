$("<%= j render partial: 'city_guides/show_place_edit', locals: {place: @place, city_guide_place: @city_guide_place} %>").insertBefore($('#new-place'));

if ($('.place-list > li').length > 4) {
	$('#new-place').hide();
	$("<%= j render partial: 'city_guides/cityguide_full' %>").appendTo($('ul.place-list')).fadeIn();
}
$('#blackbooks').append("<%= j render partial: 'resource_container', locals: {resources: @friends_black_books} %>");
$('#places').append("<%= j render partial: 'resource_container', locals: {resources: @places} %>");

<% if @friends_black_books.next_page  %>
  $('.user-blackbooks').find('.pagination').replaceWith('<%= j will_paginate @friends_black_books %>');
<% else %>
  $('.user-blackbooks').find('.see-more').remove();
<% end %>

<% if @places.next_page %>
	$('.user-places').find('.pagination').replaceWith('<%= j will_paginate @places %>');
<% else %>
	$('.user-places').find('.see-more').remove();
<% end %>